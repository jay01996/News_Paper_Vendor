        <activity android:name=".Support" />
        <activity android:name=".Testimonial" />
        <activity android:name=".Setting" />
        <activity android:name=".Monthly_Statement" />
        <activity android:name=".Stop_delivery" />
        <activity android:name=".Coupon_Expiry" />
