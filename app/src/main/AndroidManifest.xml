        <activity android:name=".Customer" />
        <activity android:name=".AddCustomer" />
        <activity android:name=".UserProfile" />
        <activity android:name=".BankDetails" />
        <activity android:name=".NewspaperMagazine" />
        <activity android:name=".Area" />
        <activity android:name=".Holidays" />
        <activity android:name=".Loan" />
        <activity android:name=".ManageStopDelivery" />
        <activity
            android:name=".PreviewCustomer"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Bill_Details"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".MonthlyReport" />
        <activity android:name=".StopDeliveryDetail" />
        <activity android:name=".ManageHolidays"></activity>
    </application>

</manifest>